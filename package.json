{
  "name": "openai-moderation-discord-bot",
  "module": "main.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/main.ts",
    "lint": "biome check src && tsc --noEmit",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate --config ./src/database/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ./src/database/drizzle.config.ts",
    "db:update": "bun run db:generate && bun run db:migrate",
    "db:studio": "drizzle-kit studio --config ./src/database/drizzle.config.ts",
    "drizzle": "drizzle-kit --config ./src/database/drizzle.config.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@types/bun": "1.2.23",
    "@types/pg": "8.15.5",
    "discord.js": "14.22.1",
    "drizzle-kit": "0.31.5",
    "husky": "9.1.7",
    "lint-staged": "16.2.3",
    "pino-pretty": "13.1.1",
    "tsx": "4.20.6"
  },
  "peerDependencies": {
    "typescript": "5.9.3"
  },
  "dependencies": {
    "drizzle-orm": "0.44.6",
    "humanize-ms": "2.0.0",
    "knub": "31.0.0",
    "openai": "6.1.0",
    "pg": "8.16.3",
    "pino": "10.0.0",
    "zod": "4.1.11"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "biome check"
  }
}
